<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="Rule" stopProcessing="true">
                    <match url="^(.*)$" ignoreCase="false" />
                        <conditions>
                            <add input="{REQUEST_FILENAME}" matchType="IsFile" ignoreCase="false" negate="true" />
                            <add input="{REQUEST_FILENAME}" matchType="IsDirectory" ignoreCase="false" negate="true" />
                            <add input="{URL}" pattern="^/favicon.ico$" ignoreCase="false" negate="true" />
                        </conditions>
                        <action type="Rewrite" url="index.php/{R:1}" appendQueryString="true" />
                </rule>
				<!-- <rule name="Redirect to WWW site"> -->
				  <!-- <match url=".*" /> -->
				  <!-- <conditions logicalGrouping="MatchAny"> -->
					<!-- <add input="acessosfcul.cloudns.cc" pattern="^(www\.)(.*)$" negate="true" /> -->
				  <!-- </conditions> -->
				  <!-- <action type="Redirect" url="https://www.acessosfcul.cloudns.cc/{R:0}" redirectType="Permanent" /> -->
				<!-- </rule> -->
				    
          <rule name="Redirect rquests to default azure websites domain" stopProcessing="true">
            <match url="(.*)" />  
            <conditions logicalGrouping="MatchAny">
              <add input="acessosfcul.cloudns.cc" pattern="^controlodeacessosuk\.azurewebsites\.net$" />
            </conditions>
            <action type="Redirect" url="http://www.acessosfcul.cloudns.cc/{R:0}" />  
          </rule>  
 
            </rules>
        </rewrite>
    </system.webServer>
</configuration>